{
  "name": "YellowFruit",
  "version": "2.5.1",
  "description": "QB Stats Program",
  "author": {
    "name": "Andrew Nadig"
  },
  "scripts": {
    "pack-win": "electron-packager . YellowFruit --platform=win32 --arch=all --out dist/pkg --overwrite --icon ./icons/banana --version-string.CompanyName=Andrew_Nadig",
    "pack-darwin": "electron-packager . YellowFruit --platform=darwin --arch=all --out dist/pkg --overwrite --icon ./icons/banana --version-string.CompanyName=Andrew_Nadig --app-version=\"2.5.1\" --app-bundle-id=\"com.AndrewNadig.YellowFruit\"",
    "sign-darwin": "electron-osx-sign dist/pkg/YellowFruit-darwin-x64/YellowFruit.app --platform=darwin --hardened-runtime"
  },
  "main": "app/main.js",
  "devDependencies": {
    "browserify": "^16.5.0",
    "electron": "^2.0.18",
    "electron-installer-dmg": "^3.0.0",
    "electron-notarize": "^0.1.1",
    "electron-osx-sign": "^0.4.15",
    "electron-packager": "^12.1.0",
    "electron-winstaller": "^2.6.4",
    "gulp": "^4.0.2",
    "gulp-bro": "^1.0.3",
    "gulp-concat-css": "^3.1.0",
    "gulp-react": "^3.1.0",
    "gulp-run": "^1.7.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.2",
    "reactify": "^1.1.1"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "electron-reload": "^1.2.5",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.5.0",
    "lodash": "^4.17.15",
    "material-icons": "^0.3.1",
    "materialize-css": "^1.0.0",
    "mousetrap": "^1.6.3",
    "popper.js": "^1.14.3"
  }
}
